{
    "author": {
        "name": "Stefan Charsley",
        "email": "charsleysa@gmail.com"
    },
    "name": "sql-ts",
    "description": "SQL Builder",
    "version": "5.3.0",
    "homepage": "https://github.com/charsleysa/node-sql-ts",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/charsleysa/node-sql-ts.git"
    },
    "main": "dist/cjs/",
    "module": "dist/esm/",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "pretest": "tslint --project . && tslint --project ./test",
        "test": "TS_NODE_PROJECT='test/tsconfig.json' mocha",
        "lint": "tslint --project .",
        "tsc": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
        "release:major": "npm run test && npm run tsc && npm version major -m \"chore: release\" && git push origin master --tags && npm publish",
        "release:minor": "npm run test && npm run tsc && npm version minor -m \"chore: release\" && git push origin master --tags && npm publish",
        "release:patch": "npm run test && npm run tsc && npm version patch -m \"chore: release\" && git push origin master --tags && npm publish"
    },
    "engines": {
        "node": "^12.0.0 || >=14.0.0"
    },
    "dependencies": {
        "lodash": "^4.17.20",
        "sliced": "~1.0.x",
        "tslib": "~2.0.1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.160",
        "@types/mocha": "^8.0.3",
        "@types/node": "^12.0.0",
        "mocha": "^8.1.2",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.0.2"
    },
    "mocha": {
        "reporter": "dot",
        "ui": "tdd",
        "require": "ts-node/register",
        "watch-extensions": "tsx,ts",
        "spec": "test/*.ts"
    }
}
